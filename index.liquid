<section id="about">
  <div style="margin-top:20px" class="more page-scroll" id="more">
    <a href="#about"><i class="fa fa-angle-down"></i></a>
  </div>
  <div class="container wp2">
    <h1 class="section-title">Who Is Dale Dupree</h1>
    <div class="row">
      <div class="col-md-6 wp2 delay-05s">
        <div class="movie-avatar-img">
          <img src="img/dale-copywarrior-001.jpg" alt="Dale with lightsaber">
        </div>
      </div>
      <div class="col-md-6 wp2 delay-1s">
        <h2>{{ fullname }} <span class="text-color">{{ title }}</span></h2>
        <h4 class="sub-text">{{ tagline }}</h4>
        <table class="text-dark">
          <tr>
            <td>Office</td>
            <td>&nbsp;&nbsp;&nbsp;:&nbsp;&nbsp;&nbsp;</td>
            <td><a href="tel:{{ office }}">{{ office }}</a></td>
          </tr>
          <tr>
            <td>Cell</td>
            <td>&nbsp;&nbsp;&nbsp;:&nbsp;&nbsp;&nbsp;</td>
            <td><a href="tel:{{ mobile | remove: '(' | remove: ')' | remove: ' ' | remove: '-' }}">{{ mobile }}</a></td>
          </tr>
          <tr>
            <td>Email</td>
            <td>&nbsp;&nbsp;&nbsp;:&nbsp;&nbsp;&nbsp;</td>
            <td><a href="mailto:{{ email }}">{{ email }}</a></td>
          </tr>
          <tr>
            <td>Fact</td>
            <td>&nbsp;&nbsp;&nbsp;:&nbsp;&nbsp;&nbsp;</td>
            <td>{{ fact }}</td>
          </tr>
        </table>
        <br>
        <p>{{ bio }}</p>
        <br>
        <div class="detailed">
          Daily Hive fives:&nbsp;&nbsp;
          <i class="fa fa-star"></i>
          <i class="fa fa-star"></i>
          <i class="fa fa-star"></i>
          <i class="fa fa-star"></i>
          <i class="fa fa-star-half-o"></i>
          <span>Five'ing Effectiveness &nbsp;<b>/  4.5</b></span>
        </div>
        <p></p>
      </div>
    </div>
  </div>
</section>
<section id="about-sub">
  <div class="container">
    <div class="row">
      <div class="col-lg-9 col-md-9 wp3 delay-05s">
        <h2 class="sub-title">{{ details.header }}</h2>
        <p>{{ details.body }}</p>
        <ul class="feature-list">
          {% for feature in details.features %}
          <li><em class="fa fa-{{ feature.type }}"></em> {{ feature.text }}</li>
          {% endfor %}
        </ul>
        <p><span class="text-color">//</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;So many more bullet points to add here. You can call me any time at <a href="tel:{{ mobile }}">{{ mobile }}</a> to ask about them.</p>
      </div>
      <div class="col-lg-3 col-md-3" id="counters">
        <ul class="counters wp3">
          {% for counter in counters %}
          <li>
            <span data-amount="{{ counter.count }}">0</span>
            <h5>{{ counter.subject }}</h5>
          </li>
          {% endfor %}
        </ul>
      </div>
    </div>
  </div>
</section>
<section id="features">
  <div class="container">
    <div class="row">
      <div id="expand-counters" class="col-md-6 features wp4">
        <h2 class="sub-title">{{ feature.title }}</h2>
        <ul class="features-wrap">
          {% for item in feature.percentages %}</h2>
          <li>
            <h6>{{ item.title}}</h6>
            <div data-amount="{{ item.percent }}"><span class="expand"><em><b></b>%</em></span></div>
          </li>
          {% endfor %}
        </ul>
      </div>
      <div class="col-md-6 wp4 delay-05s">
        <div class="panel-group" id="accordion">
          {% for person in testimonials %}
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title"><a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapse{{ forloop.index }}"><span class="glyphicon glyphicon-minus"></span> Testimonial: <strong style="color:#7D7D7D">{{ person.name }}</strong></a></h4>
            </div>
            <div id="collapse{{ forloop.index }}" class="panel-collapse collapse{% if forloop.first %} in{% endif %}">
              <div class="panel-body">
                <strong>{{ person.title }}</strong>
                <br/><br/>
                <em>{{ person.quote }}</em>
              </div>
            </div>
          </div>
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
</section>
<section id="actors">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="section-title">{{ does.title }}</h1>
        <div class="owl-actor">
          {% for item in does.abilities %}</h2>
          <div class="item">
            <div class="col-md-12 wp6{% if forloop.index == 2 %} delay-05s{% elsif forloop.index == 3 %} delay-1s{% endif %}">
              <div class="actor">
                <div class="avatar">
                  <img alt="img" src="{{ item.image }}">
                </div>
                <div class="details">
                  <h5><em>More Details Call</em><br/><a href="tel:{{ mobile }}">{{ mobile }}</a></h5>
                  <h5>Message Me</h5>
                  <a href="mailto:{{ email }}">Send Email Now</a>
                </div>
              </div>
              <div class="actor-info">
                <h4 class="heading">{{ item.heading }}</h4>
                <h5 class="sub-heading">{{ item.subheading }}</h5>
              </div>
            </div>
          </div>
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
</section>
